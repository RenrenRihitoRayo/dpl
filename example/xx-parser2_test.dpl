&use {std/text_io.py}

fn test(n)
    set a = 1
    set b = 0
    set c = 0

    while (:n > 0)
        set c = [:a + :b]
        set b = :a
        set a = :c
        dec n
    end

    return :b
end

catch (res) test(10000)
io:println(:res)